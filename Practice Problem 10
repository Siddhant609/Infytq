#PF-Prac-10
def string_both_ends(input_string):
	new_string = ''
	len1 = len(input_string)
	if len1 >2:
	    new_string = input_string[0:2] + input_string[len1-2:len1]
	elif len1 == 2:
	    new_string = input_string + input_string
	else:
	    new_string = '-1'
	return new_string

input_string="w3resource"
print(string_both_ends(input_string))
