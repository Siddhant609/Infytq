#PF-Tryout
def find_five_digit():
    for i in range(10000,100000):
        temp = i
        a = i//10000
        e = i % 10
        i = i//10
        d = i % 10
        i = i//10
        c = i%10
        i = i//10
        b = i%10
        if a==b+2 and b==2+c and d==c-2 and e==d+2 and c+d+e==a and a+b+c+d+e==19:
            print(temp)    

find_five_digit()
