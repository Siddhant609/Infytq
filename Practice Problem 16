#PF-Prac-16
def rotate_list(lists,num):
    output_list = []
    for item in range(len(lists) - num, len(lists)): 
        output_list.append(lists[item]) 
    for item in range(0, len(lists) - num):  
        output_list.append(lists[item]) 
          
    return output_list 

input_list= [1,2,3,4,5,6]
output_list=rotate_list(input_list,2)
print(output_list)
