#PF-Prac-11
def find_upper_and_lower(sentence):
    d={"UPPER_CASE":0, "LOWER_CASE":0}
    for c in sentence:
        if c.isupper():
           d["UPPER_CASE"]+=1
        elif c.islower():
           d["LOWER_CASE"]+=1
        else:
           pass
    result_list = [d["UPPER_CASE"], d["LOWER_CASE"]]
    return result_list

sentence="Hello world!"
print(find_upper_and_lower(sentence))
