#PF-Prac-31
def sum_of_elements(num_list,number):
    result_sum = 0
    for i in range(1,len(num_list)-1):
        if num_list[i]==number or num_list[i-1]==number or num_list[i+1]==number:
            continue
        result_sum += num_list[i]
    len1 = len(num_list)
    if num_list[len1-2] != number and num_list[len1-1] != number:
        result_sum += num_list[len1-1]
    return result_sum
      
num_list=[1,2,3,4]
number=2
print(sum_of_elements(num_list, number))
