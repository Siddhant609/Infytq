#PF-Prac-29
def exchange_list(number_list):
    #start writin your code here
    n = len(number_list)
    n1 = n//2
    number_list1 = number_list[n1:n] 
    number_list1 = number_list1[::-1]
    number_list2 = number_list[0:n1]
    number_list = number_list1 + number_list2
    return number_list
     
number_list=[1,2,3,4,5,6]
print(exchange_list(number_list))
