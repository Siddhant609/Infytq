#PF-Prac-7
def seed_no(number,ref_no):
    #start writing your code here
    temp = number
    prod =1
    while temp:
        rem = temp%10
        prod = prod*rem
        temp //=10
    if prod*number == ref_no:
        return True
    else:
        return False
        
    
number=123
ref_no=738
print(seed_no(number,ref_no))
